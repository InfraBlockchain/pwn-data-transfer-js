{
  "name": "pwn-data-input",
  "version": "0.1.0",
  "description": "PWN Data Input JS Library",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/prod/index.cjs",
  "module": "./dist/prod/index.esm.mjs",
  "umd:main": "./dist/prod/index.umd.js",
  "exports": {
    "require": "./dist/prod/index.cjs",
    "development": "./dist/dev/index.esm.js",
    "default": "./dist/prod/index.esm.js"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "microbundle watch",
    "lint": "eslint --fix \"src/**/\"",
    "lint:check": "eslint \"src/**/\"",
    "lint:ci": "yarn lint:check",
    "test": "NODE_OPTIONS=\"--max-old-space-size=12000\" NODE_ENV=test jest --verbose --coverage --logHeapUsage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:ci": "NODE_ENV=test jest --runInBand --ci --coverage --coverageDirectory=coverage --logHeapUsage",
    "build:prod": "NODE_ENV=production microbundle --tsconfig ./tsconfig.json  --format modern,cjs,umd  --output dist/prod/index.js",
    "build:dev": "NODE_ENV=development microbundle --define process.env.NODE_ENV=development --tsconfig ./tsconfig.json --generateTypes false --format modern --output dist/dev/index.js",
    "build": "rm -rf ./dist && yarn build:dev && yarn build:prod",
    "gen:docs": "rm -rf ./docs/api && typedoc --options typedoc.cjs",
    "example": "NODE_ENV=development yarn build:dev && ts-node example",
    "example:sig": "NODE_ENV=development yarn build:dev && ts-node example/sig.ts",
    "example:convert": "NODE_ENV=development yarn build:dev && ts-node example/convert.ts",
    "example:did": "NODE_ENV=development yarn build:dev && ts-node example/did.ts",
    "example:did2": "NODE_ENV=production yarn build:prod && ts-node example/did.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InfraBlockchain/pwn-data-input-js.git"
  },
  "author": "evan <evan@bc-labs.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InfraBlockchain/pwn-data-input-js/issues"
  },
  "homepage": "https://github.com/InfraBlockchain/pwn-data-input-js#readme",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-transform-define": "^2.1.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest-mock-console": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "microbundle": "^0.15.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "buffer": "6.0.3",
    "csv-parser": "^3.0.0",
    "infra-did-js": "1.0.0",
    "isodate": "^0.1.4",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "rdflib": "^2.2.33",
    "uuid": "^9.0.1"
  }
}
